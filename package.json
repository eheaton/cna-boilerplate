{
  "name": "cna-template-boilerplate",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "refresh": "rm -rf ./node_modules ./yarn.lock && yarn install",
    "create-component": "file-templatr -t react-ts-component -p ./components",
    "create-atom": "file-templatr -t react-ts-component -p ./components/atoms",
    "create-molecule": "file-templatr -t react-ts-component -p ./components/molecules",
    "create-organism": "file-templatr -t react-ts-component -p ./components/organisms",
    "create-module": "file-templatr -t react-ts-component -p ./components/modules",
    "create-layout": "file-templatr -t react-ts-component -p ./components/layouts"
  },
  "dependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "clsx": "^1.2.1",
    "next": "13.4.6",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@fgsake/file-templatr-cli": "https://github.com/fgsake/file-templatr-cli.git#1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0"
  }
}
